{
  "html": {
    "command": "vscode-html-language-server",
    "args": ["--stdio"],
    "extensionToLanguage": {
      ".html": "html",
      ".htm": "html"
    }
  },
  "css": {
    "command": "vscode-css-language-server",
    "args": ["--stdio"],
    "extensionToLanguage": {
      ".css": "css",
      ".scss": "scss",
      ".less": "less"
    }
  }
}
